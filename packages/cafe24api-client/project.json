{
  "name": "cafe24api-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/cafe24api-client/src",
  "projectType": "library",
  "targets": {
    "clear": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run clear",
        "cwd": "packages/cafe24api-client"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "cache": true,
      "outputs": [
        "{projectRoot}/dist"
      ],
      "options": {
        "command": "pnpm run build",
        "cwd": "packages/cafe24api-client"
      },
      "configurations": {
        "production": {}
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm publish --no-git-checks || echo 'Already published: cafe24api-client'",
        "cwd": "packages/cafe24api-client"
      }
    }
  },
  "tags": []
}